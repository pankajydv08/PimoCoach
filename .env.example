# Example environment variables for PimoCoach
# Copy this file to .env and fill in real values. Do NOT commit your .env with secrets.

# Speech-to-text (AssemblyAI)
ASSEMBLYAI_API_KEY=YOUR_ASSEMBLYAI_API_KEY

# Supabase (frontend)
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=YOUR_SUPABASE_ANON_KEY

# Supabase (server/service role - keep secret)
SUPABASE_SERVICE_ROLE_KEY=YOUR_SUPABASE_SERVICE_ROLE_KEY

# Local API base URL
VITE_API_URL=http://localhost:5000/api

# GitHub (optional)
GITHUB_TOKEN=YOUR_GITHUB_TOKEN
GITHUB_MODEL=gpt-4o

# Google Cloud credentials
# Point to a local JSON file with service account credentials (keep secret)
GOOGLE_APPLICATION_CREDENTIALS=path/to/your-service-account.json
GCS_BUCKET_NAME=your-gcs-bucket-name

# Google OAuth
GOOGLE_CLIENT_ID=YOUR_GOOGLE_CLIENT_ID
GOOGLE_CLIENT_SECRET=YOUR_GOOGLE_CLIENT_SECRET